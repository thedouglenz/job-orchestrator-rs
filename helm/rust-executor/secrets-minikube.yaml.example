# Minikube secrets configuration - EXAMPLE
# Copy this file to secrets-minikube.yaml and fill in your actual values
# DO NOT COMMIT secrets-minikube.yaml - it is gitignored

executor:
  image:
    repository: us-docker.pkg.dev/your-gcp-project-id/job-orchestrator/rust-executor
    tag: "0.1.0"  # Or use "latest" for development

  imagePullSecrets:
    - name: gcr-secret  # Ensure this secret exists: kubectl create secret docker-registry gcr-secret ...

# PostgreSQL credentials
postgresql:
  auth:
    password: your-secure-password
